<%@ Page Language="c#" AutoEventWireup="false" Inherits="MCIT.Web.Data.Browser" Culture="ar-eg"
    UICulture="ar-eg" %>

<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN" >
<html>
<head>
    <title></title>
    <meta content="Microsoft Visual Studio .NET 7.1" name="GENERATOR" />
    <meta content="C#" name="CODE_LANGUAGE" />
    <meta content="JavaScript" name="vs_defaultClientScript" />
    <meta content="http://schemas.microsoft.com/intellisense/ie5" name="vs_targetSchema" />
    <link rel="stylesheet" href="../css/Search_Style.css" type="text/css" />
    <style type="text/css">
        A:link
        {
            font-family: 'Arabic Transparent';
            font-weight: bold;
            color: #2D5FF2;
        }
        A:hover
        {
            font-family: 'Arabic Transparent';
            font-weight: bold;
            color: #400000;
            background-color: #ffffff;
        }
        A:visited
        {
            font-family: 'Arabic Transparent';
            font-weight: bold;
            color: #2D5FF2;
        }
        A:active
        {
            font-family: 'Arabic Transparent';
            font-weight: bold;
            color: #2D5FF2;
        }
        /* Datagrid style*/.GridSearch
        {
            border-right: #436485 1px solid;
            padding-right: 5px;
            border-top: #436485 1px solid;
            padding-left: 5px;
            padding-bottom: 5px;
            border-left: #436485 1px solid;
            padding-top: 5px;
            border-bottom: #436485 1px solid;
            border-square_arrow_flipped: square_arrow_flipped;
            direction: rtl;
            border-color: #436485;
        }
        .Header
        {
            font-weight: bold;
            font-size: small;
            color: #FFFFEE;
            direction: rtl;
            font-family: Verdana;
            height: 30px;
            background-color: #C2DDF0;
            text-align: center;
            direction: rtl;
        }
        .Header A:visited
        {
            font-weight: bold;
            font-family: Arial;
            text-decoration: underline;
            direction: rtl;
            color: #2D5FF2;
            background-color: #C2DDF0;
        }
        .Item
        {
            font-size: x-small;
            color: #000000;
            font-family: Arial;
            background-color: #C2DDF0;
            text-align: center;
            height: 25px;
            direction: rtl;
        }
        .AlternatingItem
        {
            font-size: x-small;
            color: #000000;
            font-family: Arial;
            background-color: #C2DDF0;
            text-align: center;
            height: 25px;
            direction: rtl;
        }
        .Footer
        {
            font-weight: bold;
            font-size: small;
            color: #Blue;
            font-family: Verdana;
            height: 30px;
            background-color: #C2DDF0;
            text-align: center;
            direction: rtl;
            border-right: #ffff66 1px solid;
            border-top: #ffff66 1px solid;
            border-left: #ffff66 1px solid;
            border-bottom: #ffff66 1px solid;
        }
        .Footer A:visited
        {
            font-weight: bold;
            color: #Blue;
            font-family: Arial;
            text-decoration: underline;
            direction: rtl;
            background-color: #C2DDF0;
        }
        .Pager
        {
            border-top-width: thick;
            font-weight: bold;
            border-left-width: thick;
            font-size: small;
            color: #FFFFEE;
            font-family: Verdana;
            height: 30px;
            background-color: #3781a8;
            text-align: center;
            direction: rtl;
        }
        .Text
        {
            border-right: #99ccff 1px solid;
            border-top: #99ccff 1px solid;
            border-left: #99ccff 1px solid;
            border-bottom: #99ccff 1px solid;
            direction: rtl;
        }
        .Button-Search
        {
            
            font-weight: 500;
            font-weight:bold;
            color: #1F4569;
            background-color: #C2DDF0;
            margin-right: 0px;
            width: 85px;
        }
        .AlignedItem
        {
            direction: rtl;
            text-align: right;
        }
        .InverselyAlignedItem
        {
            direction: rtl;
            text-align: left;
        }
        /* HTML elements style */HTML
        {
            direction: rtl;
        }
        body
        {
            background-color: #ffffff;
            color: #ffffff;
            direction: rtl;
        }
        TABLE
        {
            direction: inherit;
        }
        INPUT
        {
            direction: inherit;
        }
        H1
        {
            display: block;
            font-size: x-large;
            color: #3333cc;
            border-bottom: #cccccc 1px dotted;
            font-family: Arial;
        }
        H2
        {
            font-weight: bold;
            font-size: large;
            text-transform: capitalize;
            color: #3781a8;
            font-family: Verdana;
            text-align: center;
            direction: inherit;
        }
        H3
        {
            font-weight: bold;
            font-size: medium;
            text-transform: capitalize;
            color: #3781a8;
            font-family: Verdana;
            text-align: center;
            direction: inherit;
        }
        P
        {
            font-size: x-small;
            color: #3781a8;
            font-family: Arial;
            direction: inherit;
        }
        SPAN
        {
            color: #0a246a;
            font-family: Arial;
            direction: inherit;
        }
        STRONG, LABEL
        {
            font-size: x-small;
            color: #3781a8;
            font-family: Arial;
            direction: inherit;
        }
        HR
        {
            width: 100%;
            color: #add8e6;
            height: 2px;
            background-color: #add8e6;
            direction: inherit;
        }
        IMG
        {
            padding-right: 0px;
            padding-left: 0px;
            padding-bottom: 0px;
            margin: 0px;
            border-top-style: none;
            padding-top: 0px;
            border-right-style: none;
            border-left-style: none;
            border-bottom-style: none;
            direction: inherit;
        }
    </style>
</head>
<body style="background-color: #ffffff;">
    <form runat="server">
    </form>
</body>
</html>
